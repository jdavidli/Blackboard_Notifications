{
   "background": {
      "scripts": [ "js/swindust-background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/swindust-jquery.min.js", "js/jquery.getUrlVars.js", "js/swindust-func.js", "swindust.js" ],
      "matches": [ "http://blackboard.case.edu/*"],
      "run_at": "document_start"
   } ],
   "description": "Provides efficiencies and aesthetics to an otherwise boring and inefficient Blackboard Learning Management System!",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "name": "SwinDust for Swinburne Blackboard",
   "options_page": "options.html",
   "web_accessible_resources": [ "bb_img/close_mini.gif", "bb_img/stripe.png", "css/allpages.css", "css/options.css", "css/topmenu.css" ]
}
